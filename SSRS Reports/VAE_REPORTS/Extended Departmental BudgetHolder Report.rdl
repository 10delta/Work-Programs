      <Query>
        <DataSourceName>NAV</DataSourceName>
        <CommandText>SELECT        SYSTEM_USER AS UserID, ISNULL(InvoicedAmount.Amount, 0) AS [Invoiced Amount], ISNULL(BUDGETYTD.Amount, 0) AS [Budgeted Amount], 
                         ISNULL(FORECASTYTD.Amount, 0) AS [Forecasted Amount], DEPTGL.[CostCentre Code], DEPTGL.[CostCentre Description], DEPTGL.[CostCentre Parent], 
                         DEPTGL.[GL Account], DEPTGL.[GL Description], DEPTGL.[GL Parent], DEPTGL.[Account Type], ISNULL(AccruedAmount.[Committed Amount], 0) 
                         AS [Committed Amount], DEPTGL.[Account Classification], CASE DEPTGL.[Account Classification] WHEN 2 THEN ISNULL(InvoicedAmount.Amount, 0) 
                         ELSE 0 END AS [Actual Income], CASE DEPTGL.[Account Classification] WHEN 2 THEN 0 ELSE ISNULL(InvoicedAmount.Amount, 0) END AS [Actual Expenditure], 
                         CASE DEPTGL.[Account Classification] WHEN 4 THEN ISNULL(InvoicedAmount.Amount, 0) ELSE 0 END AS [Actual Salary], 
                         CASE DEPTGL.[Account Classification] WHEN 2 THEN ISNULL(BUDGETFULLYEAR.Amount, 0) ELSE 0 END AS [Budget Income Full Year], 
                         CASE DEPTGL.[Account Classification] WHEN 2 THEN 0 ELSE ISNULL(BUDGETFULLYEAR.Amount, 0) END AS [Budget Expenditure Full Year], 
                         CASE DEPTGL.[Account Classification] WHEN 4 THEN ISNULL(BUDGETFULLYEAR.Amount, 0) ELSE 0 END AS [Budget Salary Full Year], 
                         CASE DEPTGL.[Account Classification] WHEN 2 THEN ISNULL(FORECASTFULLYEAR.Amount, 0) ELSE 0 END AS [Forecast Income Full Year], 
                         CASE DEPTGL.[Account Classification] WHEN 2 THEN 0 ELSE ISNULL(FORECASTFULLYEAR.Amount, 0) END AS [Forecast Expenditure Full Year], 
                         CASE DEPTGL.[Account Classification] WHEN 4 THEN ISNULL(FORECASTFULLYEAR.Amount, 0) ELSE 0 END AS [Forecast Salary Full Year], 
                         CASE DEPTGL.[Account Classification] WHEN 2 THEN ISNULL(BUDGETYTD.Amount, 0) ELSE 0 END AS [Budget Income YTD], 
                         CASE DEPTGL.[Account Classification] WHEN 2 THEN 0 ELSE ISNULL(BUDGETYTD.Amount, 0) END AS [Budget Expenditure YTD], 
                         CASE DEPTGL.[Account Classification] WHEN 4 THEN ISNULL(BUDGETYTD.Amount, 0) ELSE 0 END AS [Budget Salary YTD], 
                         CASE DEPTGL.[Account Classification] WHEN 2 THEN ISNULL(FORECASTYTD.Amount, 0) ELSE 0 END AS [Forecast Income YTD], 
                         CASE DEPTGL.[Account Classification] WHEN 2 THEN 0 ELSE ISNULL(FORECASTYTD.Amount, 0) END AS [Forecast Expenditure YTD], 
                         CASE DEPTGL.[Account Classification] WHEN 4 THEN ISNULL(FORECASTYTD.Amount, 0) ELSE 0 END AS [Forecast Salary YTD], ISNULL(BUDGETFULLYEAR.Amount, 
                         0) AS [Budgeted Amount Full Year], ISNULL(FORECASTFULLYEAR.Amount, 0) AS [Forecast Amount Full Year], BUDGETYTD.Amount, 
                         FORECASTYTD.Amount AS Expr1
FROM            (SELECT        CostCentre.[CostCentre Code], CostCentre.[CostCentre Description], CostCentre.[CostCentre Parent], GLACCOUNT.[GL Account], 
                                                    GLACCOUNT.[GL Description], GLACCOUNT.[GL Parent], GLACCOUNT.[Account Type], GLACCOUNT.[Account Classification]
                          FROM            (SELECT        Code AS [CostCentre Code], Name AS [CostCentre Description], Parent AS [CostCentre Parent]
                                                    FROM            [VandA$Dimension Value]
                                                    WHERE        ([Dimension Code] = 'COSTCENTRE')) AS CostCentre CROSS JOIN
                                                        (SELECT        No_ AS [GL Account], Name AS [GL Description], Parent AS [GL Parent], [Account Type], [Account Classification]
                                                          FROM            [VandA$G_L Account]
                                                          WHERE        (Income_Balance = 0) AND (No_ &lt; 7500)) AS GLACCOUNT) AS DEPTGL LEFT OUTER JOIN
                             (SELECT        SUM([VandA$G_L Entry].Amount) AS Amount, [VandA$G_L Entry].[G_L Account No_] AS [GL Account], 
                                                         [VandA$G_L Entry].[Global Dimension 1 Code] AS [CostCentre Code]
                               FROM            [VandA$G_L Account] AS [VandA$G_L Account_1] INNER JOIN
                                                         [VandA$G_L Entry] ON [VandA$G_L Account_1].No_ = [VandA$G_L Entry].[G_L Account No_]
                               WHERE        ([VandA$G_L Entry].Accrual = 0) AND ([VandA$G_L Account_1].[Direct Posting] = 1) AND ([VandA$G_L Entry].[Global Dimension 1 Code] &lt;&gt; '') AND 
                                                         ([VandA$G_L Entry].[Posting Date] &gt;= @FromDate) AND ([VandA$G_L Entry].[Posting Date] &lt;= @ToDate) AND (DATEPART(hh, 
                                                         [VandA$G_L Entry].[Posting Date]) = 0) AND ([VandA$G_L Entry].[Shortcut Dimension 3 Code] = 'U')
                               GROUP BY [VandA$G_L Entry].[Global Dimension 1 Code], [VandA$G_L Entry].[G_L Account No_]) AS InvoicedAmount ON 
                         DEPTGL.[GL Account] = InvoicedAmount.[GL Account] AND DEPTGL.[CostCentre Code] = InvoicedAmount.[CostCentre Code] LEFT OUTER JOIN
                             (SELECT        SUM([VandA$Purchase Line_1].[Committed Amount (LCY)]) AS [Committed Amount], 
                                                         [VandA$Purchase Line_1].[Shortcut Dimension 1 Code] AS [CostCentre Code], [VandA$Purchase Line_1].No_ AS [GL Account]
                               FROM            [VandA$Purchase Line] AS [VandA$Purchase Line_1] INNER JOIN
                                                         [VandA$Purchase Header] AS [VandA$Purchase Header_1] ON [VandA$Purchase Line_1].[Document No_] = [VandA$Purchase Header_1].No_
                               WHERE        ([VandA$Purchase Line_1].[Shortcut Dimension 1 Code] &lt;&gt; '') AND ([VandA$Purchase Header_1].Deleted = 0) AND 
                                                         ([VandA$Purchase Header_1].[Shortcut Dimension 3 Code] = 'U')
                               GROUP BY [VandA$Purchase Line_1].[Shortcut Dimension 1 Code], [VandA$Purchase Line_1].No_) AS AccruedAmount ON 
                         DEPTGL.[GL Account] = AccruedAmount.[GL Account] AND DEPTGL.[CostCentre Code] = AccruedAmount.[CostCentre Code] LEFT OUTER JOIN
                             (SELECT        SUM(Amount) AS Amount, [Global Dimension 1 Code] AS [CostCentre Code], [G_L Account No_] AS [GL Account], [Budget Name]
                               FROM            [VandA$G_L Budget Entry]
                               WHERE        ([Global Dimension 1 Code] &lt;&gt; '') AND ([Budget Name] =
                                                             (SELECT        [Revised Budget]
                                                               FROM            [VandA$CultureCore Setup])) AND (Date &lt;= @ToDate) AND (Date &gt;= @FromDate) AND ([Budget Dimension 1 Code] = 'U')
                               GROUP BY [Global Dimension 1 Code], [G_L Account No_], [Budget Name]) AS BUDGETYTD ON DEPTGL.[GL Account] = BUDGETYTD.[GL Account] AND 
                         DEPTGL.[CostCentre Code] = BUDGETYTD.[CostCentre Code] LEFT OUTER JOIN
                             (SELECT        SUM(Amount) AS Amount, [Global Dimension 1 Code] AS [CostCentre Code], [G_L Account No_] AS [GL Account], [Budget Name]
                               FROM            [VandA$G_L Budget Entry] AS [VandA$G_L Budget Entry_2]
                               WHERE        ([Global Dimension 1 Code] &lt;&gt; '') AND ([Budget Name] =
                                                             (SELECT        Forecast
                                                               FROM            [VandA$CultureCore Setup] AS [VandA$CultureCore Setup_2])) AND (Date &lt;= @ToDate) AND (Date &gt;= @FromDate) AND 
                                                         ([Budget Dimension 1 Code] = 'U')
                               GROUP BY [Global Dimension 1 Code], [G_L Account No_], [Budget Name]) AS FORECASTYTD ON DEPTGL.[GL Account] = FORECASTYTD.[GL Account] AND 
                         DEPTGL.[CostCentre Code] = FORECASTYTD.[CostCentre Code] LEFT OUTER JOIN
                             (SELECT        SUM(Amount) AS Amount, [Global Dimension 1 Code] AS [CostCentre Code], [G_L Account No_] AS [GL Account], [Budget Name]
                               FROM            [VandA$G_L Budget Entry] AS [VandA$G_L Budget Entry_1]
                               WHERE        ([Global Dimension 1 Code] &lt;&gt; '') AND ([Budget Name] =
                                                             (SELECT        Forecast
                                                               FROM            [VandA$CultureCore Setup] AS [VandA$CultureCore Setup_1])) AND (Date &lt;=
                                                             (SELECT        [Portal Reporting End Date]
                                                               FROM            [VandA$CultureCore Setup] AS [VandA$CultureCore Setup_1])) AND (Date &gt;= @FromDate) AND ([Budget Dimension 1 Code] = 'U')
                               GROUP BY [Global Dimension 1 Code], [G_L Account No_], [Budget Name]) AS FORECASTFULLYEAR ON 
                         DEPTGL.[GL Account] = FORECASTFULLYEAR.[GL Account] AND DEPTGL.[CostCentre Code] = FORECASTFULLYEAR.[CostCentre Code] LEFT OUTER JOIN
                             (SELECT        SUM(Amount) AS Amount, [Global Dimension 1 Code] AS [CostCentre Code], [G_L Account No_] AS [GL Account], [Budget Name]
                               FROM            [VandA$G_L Budget Entry] AS [VandA$G_L Budget Entry_1]
                               WHERE        ([Global Dimension 1 Code] &lt;&gt; '') AND ([Budget Name] =
                                                             (SELECT        [Revised Budget]
                                                               FROM            [VandA$CultureCore Setup] AS [VandA$CultureCore Setup_1])) AND (Date &lt;=
                                                             (SELECT        [Portal Reporting End Date]
                                                               FROM            [VandA$CultureCore Setup] AS [VandA$CultureCore Setup_1])) AND (Date &gt;=
                                                             (SELECT        [Portal Reporting Start Date]
                                                               FROM            [VandA$CultureCore Setup] AS [VandA$CultureCore Setup_1])) AND ([Budget Dimension 1 Code] = 'U')
                               GROUP BY [Global Dimension 1 Code], [G_L Account No_], [Budget Name]) AS BUDGETFULLYEAR ON 
                         DEPTGL.[GL Account] = BUDGETFULLYEAR.[GL Account] AND DEPTGL.[CostCentre Code] = BUDGETFULLYEAR.[CostCentre Code]
ORDER BY DEPTGL.[CostCentre Code]</CommandText>
        <QueryParameters>
          <QueryParameter Name="@FromDate">
            <Value>=Parameters!FromDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@ToDate">
            <Value>=Parameters!ToDate.Value</Value>
          </QueryParameter>
        </QueryParameters>
      </Query>
    </DataSet>
    <DataSet Name="CostCentre">
      <Fields>
        <Field Name="Dimension_Value_Code">
          <DataField>Dimension Value Code</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Name">
          <DataField>Name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>NAV</DataSourceName>
        <CommandText>SELECT     [VandA$User Dimension Reporting].[Dimension Value Code], 
                      [VandA$User Dimension Reporting].[Dimension Value Code] + ' : ' + [VandA$Dimension Value].Name AS Name
FROM         [VandA$User Dimension Reporting] INNER JOIN
                      [VandA$Dimension Value] ON [VandA$User Dimension Reporting].[Dimension Value Code] = [VandA$Dimension Value].Code AND 
                      [VandA$User Dimension Reporting].[Dimension Code] = [VandA$Dimension Value].[Dimension Code]
WHERE     ([VandA$User Dimension Reporting].[Dimension Code] = 'COSTCENTRE') AND ([VandA$User Dimension Reporting].[User] =
                          (SELECT     [User ID]
                            FROM          [VandA$User Setup]
                            WHERE      ([Windows Login] = SYSTEM_USER)))
UNION
SELECT     '' AS [Dimension Value Code], '&lt;Blank&gt;' AS Name</CommandText>
      </Query>
    </DataSet>
    <DataSet Name="ReportDates">
      <Fields>
        <Field Name="Portal_Reporting_Start_Date">
          <DataField>Portal Reporting Start Date</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
        <Field Name="Portal_Reporting_End_Date">
          <DataField>Portal Reporting End Date</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>NAV</DataSourceName>
        <CommandText>SELECT     [Portal Reporting Start Date], [Portal Reporting End Date]
FROM         [VandA$CultureCore Setup]</CommandText>
      </Query>
    </DataSet>
  </DataSets>
