        </Field>
        <Field Name="User">
          <DataField>User</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>NAV</DataSourceName>
        <CommandText>SELECT     [Dimension Value Code], [User]
FROM         dbo.[VandA$User Dimension Reporting]
WHERE     ([Dimension Code] = 'PROJECT') AND ([User] =
                          (SELECT     [User ID]
                            FROM          dbo.[VandA$User Setup]
                            WHERE      ([Windows Login] = SYSTEM_USER)))</CommandText>
      </Query>
