          <QueryParameter Name="@DEPARTMENT">
            <Value>=Parameters!DEPARTMENT.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@Project">
            <Value>=Parameters!Project.Value</Value>
          </QueryParameter>
        </QueryParameters>
      </Query>
