      <Query>
        <DataSourceName>NAV</DataSourceName>
        <CommandText>SELECT        SYSTEM_USER AS UserID, ISNULL(BFInvoicedAmount.Amount, 0) AS [BF Invoiced Amount], ISNULL(YTDInvoicedAmount.Amount, 0) AS [YTD Invoiced Amount], 
                         ISNULL(BUDGET.Amount, 0) AS [Budgeted Amount], ISNULL(FORECAST.Amount, 0) AS [Forecast Amount], DEPTGL.[Project Code], DEPTGL.[Project Description], 
                         DEPTGL.[Project Parent], DEPTGL.[GL Description], DEPTGL.[GL Parent], DEPTGL.[Account Type], ISNULL(AccruedAmount.[Committed Amount], 0) 
                         AS [Committed Amount], DEPTGL.[Account Classification], CASE DEPTGL.[Account Classification] WHEN 2 THEN ISNULL(BFInvoicedAmount.Amount, 0) 
                         ELSE 0 END AS [BF Actual Income], CASE DEPTGL.[Account Classification] WHEN 2 THEN 0 ELSE ISNULL(BFInvoicedAmount.Amount, 0) 
                         END AS [BF Actual Expenditure], CASE DEPTGL.[Account Classification] WHEN 2 THEN ISNULL(YTDInvoicedAmount.Amount, 0) ELSE 0 END AS [YTD Actual Income], 
                         CASE DEPTGL.[Account Classification] WHEN 2 THEN 0 ELSE ISNULL(YTDInvoicedAmount.Amount, 0) END AS [YTD Actual Expenditure], 
                         CASE DEPTGL.[Account Classification] WHEN 2 THEN ISNULL(BUDGET.Amount, 0) ELSE 0 END AS [Budget Income], 
                         CASE DEPTGL.[Account Classification] WHEN 2 THEN 0 ELSE ISNULL(BUDGET.Amount, 0) END AS [Budget Expenditure], 
                         CASE DEPTGL.[Account Classification] WHEN 2 THEN ISNULL(FORECAST.Amount, 0) ELSE 0 END AS [Forecast Income], 
                         CASE DEPTGL.[Account Classification] WHEN 2 THEN 0 ELSE ISNULL(FORECAST.Amount, 0) END AS [Forecast Expenditure], DEPTGL.[GL Account]
FROM            (SELECT        Project.[Project Code], Project.[Project Description], Project.[Project Parent], GLACCOUNT.[GL Account], GLACCOUNT.[GL Description], 
                                                    GLACCOUNT.[GL Parent], GLACCOUNT.[Account Type], GLACCOUNT.[Account Classification]
                          FROM            (SELECT        Code AS [Project Code], Name AS [Project Description], Parent AS [Project Parent]
                                                    FROM            [VandA$Dimension Value] AS [VandA$Dimension Value_1]
                                                    WHERE        ([Dimension Code] = 'Project')) AS Project CROSS JOIN
                                                        (SELECT        No_ AS [GL Account], Name AS [GL Description], Parent AS [GL Parent], [Account Type], [Account Classification]
                                                          FROM            [VandA$G_L Account]
                                                          WHERE        (Income_Balance = 0) AND (No_ &lt; 7500)) AS GLACCOUNT) AS DEPTGL LEFT OUTER JOIN
                             (SELECT        SUM([VandA$G_L Entry].Amount) AS Amount, [VandA$G_L Entry].[G_L Account No_] AS [GL Account], 
                                                         [VandA$G_L Entry].[Global Dimension 2 Code] AS [Project Code]
                               FROM            [VandA$G_L Account] AS [VandA$G_L Account_1] INNER JOIN
                                                         [VandA$G_L Entry] ON [VandA$G_L Account_1].No_ = [VandA$G_L Entry].[G_L Account No_]
                               WHERE        ([VandA$G_L Entry].Accrual = 0) AND ([VandA$G_L Account_1].[Direct Posting] = 1) AND ([VandA$G_L Entry].[Global Dimension 2 Code] &lt;&gt; '') AND 
                                                         ([VandA$G_L Entry].[Posting Date] &gt;= @FromDate) AND ([VandA$G_L Entry].[Posting Date] &lt;= @ToDate) AND (DATEPART(hh, 
                                                         [VandA$G_L Entry].[Posting Date]) = 0) AND ([VandA$G_L Entry].[Shortcut Dimension 3 Code] &lt;&gt; 'P') AND 
                                                         ([VandA$G_L Entry].[Global Dimension 1 Code] IN (@DEPARTMENT))
                               GROUP BY [VandA$G_L Entry].[Global Dimension 2 Code], [VandA$G_L Entry].[G_L Account No_]) AS YTDInvoicedAmount ON 
                         DEPTGL.[GL Account] = YTDInvoicedAmount.[GL Account] AND DEPTGL.[Project Code] = YTDInvoicedAmount.[Project Code] LEFT OUTER JOIN
                             (SELECT        SUM([VandA$G_L Entry_1].Amount) AS Amount, [VandA$G_L Entry_1].[G_L Account No_] AS [GL Account], 
                                                         [VandA$G_L Entry_1].[Global Dimension 2 Code] AS [Project Code]
                               FROM            [VandA$G_L Account] AS [VandA$G_L Account_1] INNER JOIN
                                                         [VandA$G_L Entry] AS [VandA$G_L Entry_1] ON [VandA$G_L Account_1].No_ = [VandA$G_L Entry_1].[G_L Account No_]
                               WHERE        ([VandA$G_L Entry_1].Accrual = 0) AND ([VandA$G_L Account_1].[Direct Posting] = 1) AND ([VandA$G_L Entry_1].[Global Dimension 2 Code] &lt;&gt; '') AND 
                                                         ([VandA$G_L Entry_1].[Posting Date] &lt; @FromDate) AND (DATEPART(hh, [VandA$G_L Entry_1].[Posting Date]) = 0) AND 
                                                         ([VandA$G_L Entry_1].[Shortcut Dimension 3 Code] &lt;&gt; 'P') AND ([VandA$G_L Entry_1].[Global Dimension 1 Code] IN (@DEPARTMENT))
                               GROUP BY [VandA$G_L Entry_1].[Global Dimension 2 Code], [VandA$G_L Entry_1].[G_L Account No_]) AS BFInvoicedAmount ON 
                         DEPTGL.[GL Account] = BFInvoicedAmount.[GL Account] AND DEPTGL.[Project Code] = BFInvoicedAmount.[Project Code] LEFT OUTER JOIN
                             (SELECT        SUM([VandA$Purchase Line_1].[Committed Amount (LCY)]) AS [Committed Amount], 
                                                         [VandA$Purchase Line_1].[Shortcut Dimension 2 Code] AS [Project Code], [VandA$Purchase Line_1].No_ AS [GL Account]
                               FROM            [VandA$Purchase Line] AS [VandA$Purchase Line_1] INNER JOIN
                                                         [VandA$Purchase Header] AS [VandA$Purchase Header_1] ON [VandA$Purchase Line_1].[Document No_] = [VandA$Purchase Header_1].No_
                               WHERE        ([VandA$Purchase Line_1].[Shortcut Dimension 2 Code] &lt;&gt; '') AND ([VandA$Purchase Line_1].[Shortcut Dimension 1 Code] IN (@DEPARTMENT)) AND 
                                                         ([VandA$Purchase Header_1].Deleted = 0)
                               GROUP BY [VandA$Purchase Line_1].[Shortcut Dimension 2 Code], [VandA$Purchase Line_1].No_) AS AccruedAmount ON 
                         DEPTGL.[GL Account] = AccruedAmount.[GL Account] AND DEPTGL.[Project Code] = AccruedAmount.[Project Code] LEFT OUTER JOIN
                             (SELECT        SUM(Amount) AS Amount, [Global Dimension 2 Code] AS [Project Code], [G_L Account No_] AS [GL Account], [Budget Name]
                               FROM            [VandA$G_L Budget Entry]
                               WHERE        ([Global Dimension 2 Code] &lt;&gt; '') AND ([Budget Name] =
                                                             (SELECT        [Revised Budget]
                                                               FROM            [VandA$CultureCore Setup])) AND (Date &lt;=
                                                             (SELECT        [Portal Reporting End Date]
                                                               FROM            [VandA$CultureCore Setup] AS [VandA$CultureCore Setup_1])) AND (Date &gt;= @FromDate) AND 
                                                         ([Global Dimension 1 Code] IN (@DEPARTMENT))
                               GROUP BY [Global Dimension 2 Code], [G_L Account No_], [Budget Name]) AS BUDGET ON DEPTGL.[GL Account] = BUDGET.[GL Account] AND 
                         DEPTGL.[Project Code] = BUDGET.[Project Code] LEFT OUTER JOIN
                             (SELECT        SUM(Amount) AS Amount, [Global Dimension 2 Code] AS [Project Code], [G_L Account No_] AS [GL Account], [Budget Name]
                               FROM            [VandA$G_L Budget Entry] AS [VandA$G_L Budget Entry_1]
                               WHERE        ([Global Dimension 2 Code] &lt;&gt; '') AND ([Budget Name] =
                                                             (SELECT        Forecast
                                                               FROM            [VandA$CultureCore Setup] AS [VandA$CultureCore Setup_2])) AND (Date &lt;=
                                                             (SELECT        [Portal Reporting End Date]
                                                               FROM            [VandA$CultureCore Setup] AS [VandA$CultureCore Setup_1])) AND (Date &gt;= @FromDate) AND 
                                                         ([Global Dimension 1 Code] IN (@DEPARTMENT))
                               GROUP BY [Global Dimension 2 Code], [G_L Account No_], [Budget Name]) AS FORECAST ON DEPTGL.[GL Account] = FORECAST.[GL Account] AND 
                         DEPTGL.[Project Code] = FORECAST.[Project Code]
WHERE        (DEPTGL.[Project Code] IN (@Project)) AND (NOT (DEPTGL.[GL Account] LIKE '601%'))</CommandText>
        <QueryParameters>
          <QueryParameter Name="@FromDate">
            <Value>=Parameters!FromDate.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@ToDate">
            <Value>=Parameters!ToDate.Value</Value>
